{
  "name": "list-to-sentence",
  "version": "1.0.7",
  "description": "Convert a list of words to a sentence",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etc-tiago/list-to-sentence.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@etcdigital/get-number-from-string": "^1.0.2",
    "bunchee": "6.3.2"
  },
  "homepage": "https://usertimeline.com",
  "main": "./dist/index.js",
  "scripts": {
    "build": "bunchee"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["readme.md", "dist"],
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false
}
